CREATE TABLE address(
address_id BIGSERIAL NOT NULL,
country VARCHAR(30) ,
city VARCHAR(25),
street VARCHAR(35) NOT NULL,
house_number INT NOT NULL,
	flat_number INT,
	PRIMARY KEY (address_id))

	INSERT INTO address(address_id, country, city, street, house_number, flat_number)
